<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="180.0" minWidth="300.0" style="-fx-background-color: #18181C;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="Conference Details" textFill="WHITE">
         <font>
            <Font name="Calibri" size="20.0" />
         </font>
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </Label>
      <TextField promptText="Conference name" style="-fx-border-color: #363648; -fx-background-color: 0; -fx-border-width: 0 0 3 0; -fx-text-fill: WHITE;">
         <VBox.margin>
            <Insets left="15.0" right="15.0" top="5.0" />
         </VBox.margin>
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <TextField promptText="Something else" style="-fx-border-color: #363648; -fx-background-color: 0; -fx-border-width: 0 0 3 0; -fx-text-fill: WHITE;">
         <VBox.margin>
            <Insets left="15.0" right="15.0" top="20.0" />
         </VBox.margin>
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <HBox alignment="CENTER_RIGHT">
         <children>
            <Button mnemonicParsing="false" style="-fx-border-style: solid; -fx-border-width: 0; -fx-border-radius: none; -fx-background-color: #363648;" text="Cancel" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="Calibri" size="14.0" />
               </font>
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" style="-fx-border-style: solid; -fx-border-width: 0; -fx-border-radius: none; -fx-background-color: #363648;" text="Create" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="Calibri" size="14.0" />
               </font>
            </Button>
         </children>
         <VBox.margin>
            <Insets right="15.0" top="10.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
