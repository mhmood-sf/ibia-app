<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="ibia.app.controllers.ResolutionsListItem">
   <children>
      <HBox alignment="CENTER_LEFT" maxWidth="250.0" HBox.hgrow="ALWAYS">
         <children>
            <Text fx:id="delegate" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="USA">
               <font>
                  <Font size="14.0" />
               </font>
               <HBox.margin>
                  <Insets left="15.0" />
               </HBox.margin>
            </Text>
         </children>
         <HBox.margin>
            <Insets bottom="5.0" />
         </HBox.margin>
      </HBox>
      <HBox alignment="CENTER_LEFT" HBox.hgrow="ALWAYS">
         <children>
            <Text fx:id="topic" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Topic 1">
               <font>
                  <Font size="14.0" />
               </font>
               <HBox.margin>
                  <Insets left="15.0" />
               </HBox.margin>
            </Text>
         </children>
         <HBox.margin>
            <Insets bottom="5.0" />
         </HBox.margin>
      </HBox>
      <Button mnemonicParsing="false" onMouseClicked="#deleteReso" style="-fx-background-color: #6E6C78;" text="Delete" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <HBox.margin>
            <Insets bottom="5.0" right="5.0" />
         </HBox.margin>
      </Button>
   </children>
</HBox>
