<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox alignment="TOP_CENTER" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="ibia.app.controllers.CommitteeListItem">
   <children>
      <Text fill="WHITE" onMouseClicked="#navigate" onMouseEntered="#hoverEffectOn" onMouseExited="#hoverEffectOff" strokeType="OUTSIDE" strokeWidth="0.0" text="Committee Name">
         <font>
            <Font size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Text>
      <Text fx:id="id" fill="#544646" strokeType="OUTSIDE" strokeWidth="0.0" text="#Committee Id">
         <font>
            <Font size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </Text>
   </children>
   <padding>
      <Insets bottom="5.0" />
   </padding>
</HBox>
